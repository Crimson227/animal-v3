<tr class="table-entry">
    <th id="{{animalID}}">{{animalName}}</th>

    {{#if patronID}}
    <th>{{firstName}} {{lastName}}</th>
    {{else}}
    <th>未知人员</th>
    {{/if}}
    <th>{{fosteredOrAdopted}}</th>
    <th>{{date}}</th>

    {{#if patronID}}
    <th class="hidden">{{patronID}}</th>
    {{else}}

    {{/if}}
    <th class="hidden">0</th>
</tr>